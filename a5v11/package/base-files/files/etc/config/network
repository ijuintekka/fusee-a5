config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth0'
	option proto 'dhcp'

config device 'lan_dev'
	option name 'eth0'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '0'